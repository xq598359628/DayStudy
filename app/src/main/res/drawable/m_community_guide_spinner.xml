<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background"
        android:drawable="@color/gh_cm_transparent_2"/>
    <item android:drawable="@drawable/m_community_guide_spinner_inner"/>
    <item>
        <rotate
            android:fromDegrees="0"
            android:interpolator="@android:anim/linear_interpolator"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="360">
            <bitmap
                android:antialias="true"
                android:filter="true"
                android:src="@drawable/m_community_guide_spinner_outer"/>
        </rotate>
    </item>
</layer-list>